use Test;
use Ecosystem;

plan 3;

my $ec := Ecosystem.new;
isa-ok $ec, Ecosystem;

my $identity := $ec.find-identities("eigenstates", :auth<zef:lizmat>, :ver<0.0.9>).head;
is $identity,
  "eigenstates:ver<0.0.9>:auth<zef:lizmat>",
  'is identity ok';

is $ec.identities{$identity}<source-url>,
  "https://raw.githubusercontent.com/raku/REA/main/archive/E/eigenstates/eigenstates:ver\%3C0.0.9\%3E:auth\%3Czef:lizmat\%3E.tar.gz",
  'is source-url ok?';

